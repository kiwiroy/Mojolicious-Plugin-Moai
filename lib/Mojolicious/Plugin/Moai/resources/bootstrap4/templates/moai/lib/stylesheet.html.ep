<%
    my $version = stash( 'version' ) // moai->config->{version} // die "No Moai UI library version specified";
    my @urls = (
        sprintf( '//cdn.jsdelivr.net/npm/bootstrap@%s/dist/css/bootstrap.min.css', $version ),
    );
%>
% for my $url ( @urls ) {
    %= stylesheet $url, moai->integrity($url)
% }
