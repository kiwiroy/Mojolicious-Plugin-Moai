<%
    my $version = stash( 'version' ) // moai->config->{version} // die "No Moai UI library version specified";
    my @urls = (
        '//cdn.jsdelivr.net/npm/jquery@3.4.1/dist/jquery.min.js',
        '//cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js',
        sprintf( '//cdn.jsdelivr.net/npm/bootstrap@%s/dist/js/bootstrap.bundle.min.js', $version ),
    );
%>
% for my $url ( @urls ) {
    %= javascript $url, moai->integrity($url)
% }
